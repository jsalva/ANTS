@ARTICLE{Avants2007a,
  author = {Brian B Avants and Hallam Hurt and Joan M Giannetta and Charles L
	Epstein and David M Shera and Hengyi Rao and Jiongjiong Wang and
	James C Gee},
  title = {Effects of heavy in utero cocaine exposure on adolescent caudate
